{
	"name": "ares-nest-starter",
	"description": "A starter pack for nestjs server side project.",
	"main": "main.js",
	"scripts": {
		"clean": "rimraf dist",
		"build": "nest build",
		"start": "node dist/main.js",
		"dev": "nest start --watch",
		"lint": "tslint --fix -p tsconfig.json -c tslint.json",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"test": "jest",
		"test:cov": "jest --coverage",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dante-teo/ares-nest-starter.git"
	},
	"author": "Dante Teo",
	"license": "WTFPL",
	"bugs": {
		"url": "https://github.com/dante-teo/ares-nest-starter/issues"
	},
	"homepage": "https://github.com/dante-teo/ares-nest-starter#readme",
	"dependencies": {
		"@nestjs/common": "^6.8.5",
		"@nestjs/core": "^6.8.5",
		"@nestjs/platform-express": "^6.8.5",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.5.3"
	},
	"devDependencies": {
		"@nestjs/cli": "^6.11.2",
		"@nestjs/schematics": "^6.7.0",
		"@nestjs/testing": "^6.8.5",
		"@types/express": "^4.17.1",
		"@types/jest": "^24.0.21",
		"@types/node": "^12.12.0",
		"@types/supertest": "^2.0.8",
		"jest": "^24.9.0",
		"prettier": "^1.18.2",
		"rimraf": "^3.0.0",
		"supertest": "^4.0.2",
		"ts-jest": "^24.1.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.4.1",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^5.20.0",
		"tslint-config-level5": "^1.1.2",
		"typescript": "^3.6.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
