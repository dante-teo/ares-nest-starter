{
	"name": "ares-nest-starter",
	"version": "1.0.0",
	"description": "A starter pack for nestjs server side project.",
	"main": "index.js",
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint '{src,app,libs,test}/**/*.ts' --fix",
		"clean": "rimraf dist",
		"build": "nest build",
		"dev": "nest start --watch",
		"start": "node dist/index.js",
		"test": "jest",
		"coverage": "jest --coverage",
		"e2e": "jest --config ./test/jest-e2e.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dante-teo/ares-nest-starter.git"
	},
	"author": "Dante Teo",
	"license": "WTFPL",
	"bugs": {
		"url": "https://github.com/dante-teo/ares-nest-starter/issues"
	},
	"homepage": "https://github.com/dante-teo/ares-nest-starter#readme",
	"dependencies": {
		"@nestjs/common": "^6.11.5",
		"@nestjs/core": "^6.11.5",
		"@nestjs/platform-express": "^6.11.5",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.5.4"
	},
	"devDependencies": {
		"@nestjs/cli": "^6.14.1",
		"@nestjs/schematics": "^6.9.3",
		"@nestjs/testing": "^6.11.5",
		"@types/express": "^4.17.2",
		"@types/jest": "^25.1.1",
		"@types/node": "^13.7.0",
		"@types/supertest": "^2.0.8",
		"@typescript-eslint/eslint-plugin": "^2.19.0",
		"@typescript-eslint/parser": "^2.19.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"jest": "^25.1.0",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.1",
		"supertest": "^4.0.2",
		"ts-jest": "^25.2.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.6.2",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.7.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
