{
  "name": "ares-nest-starter",
  "version": "1.0.0",
  "description": "A starter pack for nestjs server side project.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --fix -p tsconfig.json -c tslint.json",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "concurrently --handle-input \"wait-on dist/index.js && nodemon\" \"tsc -w -p tsconfig.build.json\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "author": "Dante Teo",
  "license": "WTFPL",
  "dependencies": {
    "@nestjs/common": "^6.2.0",
    "@nestjs/core": "^6.2.0",
    "@nestjs/platform-express": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.2.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-level5": "^1.1.1",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
